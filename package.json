{
  "name": "ant-design-pro",
  "version": "5.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "openapi": "umi openapi",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit",
    "update:deps": "yarn upgrade-interactive --latest"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.20.1",
    "@ant-design/pro-descriptions": "^1.10.69",
    "@ant-design/pro-form": "^1.66.0",
    "@ant-design/pro-layout": "^6.35.0",
    "@ant-design/pro-table": "^2.73.0",
    "@apollo/client": "^3.6.2",
    "@monaco-editor/react": "^4.4.4",
    "@umijs/route-utils": "^2.0.5",
    "antd": "^4.20.2",
    "classnames": "^2.2.6",
    "graphql": "^16.4.0",
    "gravatar": "^1.8.2",
    "highlight.js": "^11.5.1",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.11",
    "marked": "^4.0.14",
    "moment": "^2.29.3",
    "monaco-editor": "^0.33.0",
    "omit.js": "^2.0.2",
    "react": "^18.1.0",
    "react-dev-inspector": "^1.8.0",
    "react-dom": "^18.1.0",
    "react-helmet-async": "^1.3.0",
    "react-transition-group": "^4.4.2",
    "screenfull": "^6.0.1",
    "ua-parser-js": "^1.0.2",
    "umi": "^3.5.23",
    "umi-serve": "^1.9.10"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.2",
    "@types/express": "^4.17.0",
    "@types/gravatar": "^1.8.3",
    "@types/history": "^5.0.0",
    "@types/jest": "^27.4.1",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.182",
    "@types/marked": "^4.0.3",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-helmet": "^6.1.5",
    "@types/react-transition-group": "^4.4.3",
    "@types/ua-parser-js": "^0.7.36",
    "@umijs/fabric": "^2.10.2",
    "@umijs/openapi": "^1.4.4",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/plugin-openapi": "^1.3.2",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-dumi": "^1.1.40",
    "@umijs/preset-react": "^1.8.30",
    "@umijs/yorkie": "^2.0.3",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.4.0",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.14.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "express": "^4.18.1",
    "gh-pages": "^3.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^12.4.1",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.6.2",
    "puppeteer-core": "^13.7.0",
    "stylelint": "^14.8.1",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "gitHooks": {
    "commit-msg": "fabric verify-commit"
  }
}
